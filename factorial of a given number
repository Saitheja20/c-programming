//C Program to find factorial of a given number

#include <stdio.h>

int main() {
    int num,result;
    printf("eneter a number\n");
    scanf("%d",&num);
    result=factorial(num);
    printf("%d\n",result);
}
int factorial(int n){
    int i,factorial=1;
    for(i=1;i<=n;i++){
        factorial=factorial*i;

    }
   // printf("%d\n",factorial);
    return factorial;
}
